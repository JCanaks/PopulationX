type User {
    id: ID! @id
    name: String!
    email: String! @unique
    password: String!
}

type Location {
    id: ID! @id
    name: String!
    noOfFemales: Int!
    noOfMales: Int!
    totalResidents: Int!
    parentLocation: Location @relation(name:"ParentLocation")
    nestedLocations: [Location] @relation(name: "NestedLocations", onDelete: CASCADE)
}

